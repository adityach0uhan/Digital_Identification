    <%- include("common/navigationBar.ejs") %>






<!-- table  -->
<h2>User data view page</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th scope="col">Details </th>
            <th scope="col">User Infromation</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <%  x.forEach((y)=>{ %>
        <th scope="row">Full Name : </th>
        <td><%= y.Username%></td>
      </tr>
      <tr>
        <th scope="row">Branch :</th>
        <td> <%= y.branch%></td>
      </tr>
      <tr>
        <th scope="row">Roll Number :</th>
        <td>  <%= y.rollNum %></td>
      </tr>
      <tr>
        <th scope="row">Enrollment Number :</th>
        <td>  <%= y.enrollmentNum %></td>
      </tr>
      <tr>
        <th scope="row">Gender :</th>
        <td>  <%= y.gender %></td>
      </tr>
      <tr>
        <th scope="row">Address :</th>
        <td>  <%= y.Address%></td>
      </tr>

      
    </tbody>
  </table>
            <!-- table  -->


    
       
        
        <% })%>
    

    
